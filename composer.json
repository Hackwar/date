{
    "name": "joomla/date",
    "type": "joomla-package",
    "description": "Joomla Date Package",
    "keywords": ["joomla", "framework", "date"],
    "homepage": "https://github.com/joomla-framework/date",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.3.10"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Date\\": "src/",
            "Joomla\\Date\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
